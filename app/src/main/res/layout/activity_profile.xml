<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f16461"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="프로필 등록"
            android:textAlignment="viewStart"
            android:textColor="#ffffff"
            android:textSize="23sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <ImageButton
            android:id="@+id/closeButton"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/ic_close" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="195dp"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:scrollbars="horizontal">

            <LinearLayout
                android:id="@+id/linear_layout1"
                android:layout_width="44dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/add_picture1"
                    android:layout_width="88dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:background="@drawable/ticket_add" />

                <ImageView
                    android:id="@+id/add_picture2"
                    android:layout_width="88dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    app:srcCompat="@drawable/ticket_add" />

                <ImageView
                    android:id="@+id/add_picture3"
                    android:layout_width="88dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    app:srcCompat="@drawable/ticket_add" />

                <ImageView
                    android:id="@+id/add_picture4"
                    android:layout_width="88dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    app:srcCompat="@drawable/ticket_add" />

                <ImageView
                    android:id="@+id/add_picture5"
                    android:layout_width="88dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="30dp"
                    app:srcCompat="@drawable/ticket_add" />
            </LinearLayout>

        </HorizontalScrollView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="닉네임"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="185dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:inputType="textPersonName"
            android:textSize="15dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="85dp"
            android:layout_height="45dp"
            android:layout_gravity="center_vertical"
            android:paddingRight="30dp"
            app:srcCompat="@drawable/check_id_button" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="나이"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@null"
            android:inputType="textPersonName"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="성별"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@null"
            android:inputType="textPersonName"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="직업"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@null"
            android:inputType="textPersonName"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/p_style"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="여행 스타일"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:gravity="left|center"
            android:text="1."
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center"
            android:text="2."
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center"
            android:text="3."
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/p_char"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="성격"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/p_interest"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="관심사"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/p_pr"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="자기소개"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#9b9b9b"
            android:textSize="15dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottomButtonLayout"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/Profile_prev"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#92b6d5  "
            android:text="이전"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <Button
            android:id="@+id/Profile_next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#92b6d5  "
            android:text="등록"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </LinearLayout>
</LinearLayout>